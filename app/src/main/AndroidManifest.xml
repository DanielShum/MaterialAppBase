<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daililol.material.appbase.example"
    android:versionCode="2"
    android:versionName="2.0" android:installLocation="auto">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="23" />

    <application
        android:name="com.daililol.material.appbase.example.ExampleApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Base.AppTheme.WindowContentTransition">
      
        <activity
            android:name="com.daililol.material.appbase.example.ExampleNavigationDrawerActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.daililol.material.appbase.example.ExampleTabbarActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.daililol.material.appbase.example.ExampleSearchBarActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.daililol.material.appbase.example.ExampleActionbarActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.daililol.material.appbase.example.ExampleCollapsingViewActivity"
            android:screenOrientation="portrait">
        </activity>
        

        
    </application>

</manifest>
